    "react-test-renderer": "^16.6.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setup/setupEnzyme.js",
    "transform": {
      "{^.+\\.js$": "babel-jest"
